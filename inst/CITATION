citHeader("To cite HeckmanEM in publications use:")

bibentry(
  bibtype = "Article",
  title        = "Heckman selection-t model: Parameter estimation via the EM-algorithm",
  author       = c(person("V{\'i}ctor Hugo Lachos"),
  		   person("Marcos Oliveira Prates"),
                   person("Dipak Kumar Dey")),
  journal = "Journal of Multivariate Analysis",
  volume="184",
  pages="104737",
  year="2021",

  textVersion  =
  paste("Victor Hugo Lachos, Marcos Oliveira Prates, Dipak Kumar Dey (2021).",
        "Heckman selection-t model: parameter estimation via the EM-algorithm.",
        "Journal of Multivariate Analysis, 184, 104737.")
)

